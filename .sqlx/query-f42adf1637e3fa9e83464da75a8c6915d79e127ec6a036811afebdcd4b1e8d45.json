  "query": "\n            SELECT status as \"status: BundleStatus\"\n            FROM (\n                SELECT status FROM pending_bundles WHERE bundle_id = $1\n                UNION ALL\n                SELECT status FROM finished_bundles WHERE bundle_id = $1\n            ) AS combined\n            LIMIT 1\n            ",
